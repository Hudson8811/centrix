								source(type="image/webp" srcset="img/picture/carousel/10.webp 1x, img/picture/carousel/10@2x.webp 2x")
								// 1х: 404px; 2x: 808px
								img(src="img/picture/carousel/10.jpg", srcset="img/picture/carousel/10@2x.jpg 2x", width="404", height="494", alt="Copenhagen")
							span.project-preview__bottom
								span.project-preview__title Copenhagen
								span.project-preview__icon
									+icon('link-arrow2')(width="24" height="23")
						a.carousel__item.project-preview.swiper-slide(href= URLS.projects.single)
							picture
								source(type="image/webp" srcset="img/picture/carousel/11.webp 1x, img/picture/carousel/11@2x.webp 2x")
								// 1х: 662px; 2x: 1324px
								img(src="img/picture/carousel/11.jpg", srcset="img/picture/carousel/11@2x.jpg 2x", width="434", height="321", alt="Mobile app")
